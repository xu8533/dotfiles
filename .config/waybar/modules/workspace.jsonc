{
  "hyprland/workspaces": {
    "persistent-workspaces": {
      "*": 10,
    },
    "sort-by-number": true,
    "on-click": "activate",
    "disable-scroll": false,
    "all-outputs": false,
    "active-only": false,
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "format": "{icon}",
    "format-icons": {
      "1": "壹",
      "2": "贰",
      "3": "叁",
      "4": "肆",
      "5": "伍",
      "6": "陆",
      "7": "柒",
      "8": "捌",
      "9": "玖",
      "10": "零",
      "video": "📽️",
      "music": "🪕",
      "media": "🎬",
      "network": "📡",
      "office": "🔭",
      "note": "📖",
      "urgent": " 🚑 ",
      "active": "🏓",
      "default": "🍄 ",
      "empty": "🚴",
      "persistent": "🀄 ",
    },
    "window-rewrite-default": "",
    "window-rewrite": {
      "title<.*YouTube.*>": "",
      "class<firefox>": "",
      "class<firefox> title<.*github.*>": "",
      "class<kitty>": "",
      "class<kitty> title<ncmpcpp>": "🪕",
    },
  },
}
