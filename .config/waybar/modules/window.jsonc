{
  "hyprland/window": {
    "format": "{title}",
    // "format": "{}",
    "max-length": 25,
    "separate-outputs": true,
    "rewrite": {
      "(.*) — Mozilla Firefox": "🌎 $1",
      "(.*) - Chromium": "🌍 $1",
      "(.*) - bash": " [$1]",
      "(.*) - NVIM": " $1",
      "(.*) - Obsidian (.*)": "📘 $1",
      "(.*)~": "  $1",
      "(.*)rofi- dmenu": " 🈯 ",
      "(.*)rofi": " 🚀 ",
      "(.*) - mpv": "  $1",
      "(.*) - Audacious": "🎶 $1",
      "(.*)musicfox": " 🥁",
      "(.*)ranger": " 🉑 ",
      "yazi: (.*)": " 🦆 $1",
      "(.*) kitty": " $1",
      "(.*) ncmpcpp": "🪕 $1",
    },
    "tooltip": true,
  },
}
