{
  "custom/media": {
    "format": "{icon} {}",
    "return-type": "json",
    "max-length": 25,
    "format-icons": {
      "spotify": " ",
      "default": " "
    },
    "escape": true,
    "exec": "$HOME/.config/scripts/mediaplayer.py 2> /dev/null",
    "on-click": "playerctl play-pause"
  }
}
