{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "green",
                    "foreground": "#193549",
                    "leading_diamond": "\ue0c7",
                    "properties": {
                        "style": "full"
                    },
                    "style": "diamond",
                    "template": " {{ .Path }} ",
                    "trailing_diamond": "\ue0c6",
                    "type": "path"
                },
                {
                    "background": "lightCyan",
                    "foreground": "#fff",
                    "leading_diamond": "\ue0c7",
                    "properties": {
                        "branch_icon": ""
                    },
                    "style": "diamond",
                    "template": " git({{ .HEAD }}) ",
                    "trailing_diamond": "\ue0c6",
                    "type": "git"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}
