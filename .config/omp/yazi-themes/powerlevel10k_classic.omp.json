{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#546E7A",
                    "foreground": "#26C6DA",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#546E7A",
                    "foreground": "#D4E157",
                    "style": "plain",
                    "template": "<#26C6DA>\ue0b1 </>{{ .HEAD }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 3
}
