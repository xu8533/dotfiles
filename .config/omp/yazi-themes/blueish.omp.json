{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#14c2dd",
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "style": "full"
                    },
                    "style": "powerline",
                    "template": " \ue5ff {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#a2c4e0",
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}
