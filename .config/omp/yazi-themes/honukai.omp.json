{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#0973C0",
                    "properties": {
                        "folder_icon": "\uf115",
                        "folder_separator_icon": " \ue0b1 ",
                        "style": "full"
                    },
                    "style": "plain",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "foreground": "#B8B80A",
                    "style": "plain",
                    "template": "<#ffffff>on git:</>{{ .HEAD }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}
